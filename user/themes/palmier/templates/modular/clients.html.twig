<div id="clients">
  <div class="container">
    <div class="section_header">
      <h3>{{ page.header.title }}</h3>
    </div>
    <div class="row">
      {% for partenaire in page.header.partenaires  %}
      <div class="col-md-2 col-sm-4 client">
          <a href="{{ partenaire.button_url }}"><img src="{{ page.media[partenaire.image].url }}" /></a>
      </div>
      {% endfor %}
      <div class="col-md-2 col-sm-4 client">
          <div class="img client2"></div>
      </div>
    </div>
  </div>
</div>
